package com.joeun.joeunmall.vo;

import java.util.Date;

/**
 * 1:1문의 VO(값 객체)
 * 
 * @author PJM
 */ 

public class InquiryTBLVO {
	
	/** 문의 번호 */
	private int inquiryIndex;
	
	/** 고객 번호 */
	private int userIndex;
	
	/** 문의 일자 */
	private Date inquiryDate;
	
	/** 문의 항목 */
	private String inquiryCategory;
	
	/** 문의 처리 상태 */
	private String inquiryState;
	
	/** 문의 제목 */
	private String inquiryTitle;
	
	/** 문의 내용 */
	private String inquiryContent;
	
	/** 문의 답변 */
	private String inquiryAnswer;
	
	public InquiryVO(int inquiryIndex, int userIndex, Date inquiryDate, String inquiryCategory, String inquiryState, String inquiryTitle, String inquiryContent, String inquiryAnswer) {
		this.inquiryIndex = inquiryIndex;
		this.userIndex = userIndex;
		this.inquiryDate = inquiryDate;
		this.inquiryCategory = inquiryCategory;
		this.inquiryState = inquiryState;
		this.inquiryTitle = inquiryTitle;
		this.inquiryContent = inquiryContent;
		this.inquiryAnswer = inquiryAnswer;
	}
	
}