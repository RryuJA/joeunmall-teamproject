public class InquiryTBLDummy {
	
	//문의번호 dummy 
	private String makeInquiryIndex(int i) {
		
		String result = "22";
		
		int month = 1 + (int)Math.random()*12;
		result += String.format("%02d", month);
		
		int date = 1 + (int)Math.random()*30;
		result += String.format("%02d", date);
		
		return result + String.format("%03d", i);
	}
	
	//고객번호 dummy
	private String makeUserindex(int i) {
		
		String result ="2022";
		
		return result + String.format("%03d", i);
	}
	
