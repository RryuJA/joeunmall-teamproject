CREATE TABLE `USER` (
	`USER_INDEX`	NUMBER(11) PRIMARY KEY,
	`USER_ID`	VARCHAR2(50)	NOT NULL UNIQUE,
	`USER_NAME`	VARCHAR2(50 CHAR)	NOT NULL,
	`USER_PW`	VARCHAR2(60)	NOT NULL,
	`USER_DATE`	DATE	SYSDATE,
	`USER_MAIL`	VARCHAR2(50 CHAR)	NULL,
	`USER_BIRTH`	DATE	NULL,
	`USER_GENDER`	enum('M','F')	NULL,
	`USER_LANDLINE`	VARCHAR(13)	NULL,
	`USER_MOBILE`	VARCHAR(13)	NULL,
	`USER_ADDRESS`	VARCHAR(50)	NULL,
	`USER_ADDRESS_DETAIL`	VARCHAR(50)	NULL,
	`USER_ADDRESS_POST`	VARCHAR(50)	NULL
);